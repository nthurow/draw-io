        <mxCell id="OSdYiM-w9uRwdhkcg_Fl-98" value="&lt;font color=&quot;#ff3333&quot;&gt;&lt;b&gt;searchForPharmacies&lt;/b&gt;&lt;/font&gt;&lt;b&gt;:&lt;/b&gt;&lt;br&gt;function searchForPharmacies(address, radius) {&lt;br&gt;&amp;nbsp; return axios.get(pharmacySearchEndpoint, {address, radius});&lt;br&gt;}" style="rounded=0;whiteSpace=wrap;html=1;align=left;verticalAlign=top;" vertex="1" parent="1">
